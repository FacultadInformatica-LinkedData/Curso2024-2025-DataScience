@prefix rr: <http://www.w3.org/ns/r2rml#>.
@prefix rml: <http://semweb.mmlab.be/ns/rml#>.
@prefix ql: <http://semweb.mmlab.be/ns/ql#>.
@prefix transit: <http://vocab.org/transit/terms/>.
@prefix xsd: <http://www.w3.org/2001/XMLSchema#>.
@prefix wgs84_pos: <http://www.w3.org/2003/01/geo/wgs84_pos#>.
@prefix ns: <http://example.org/parking#> .

<#AparcamientosPublicosTriplesMap> a rr:TriplesMap;
  rml:logicalSource [
    rml:source "csv/202625-0-aparcamientos-publicos-updated.csv";
    rml:referenceFormulation ql:CSV
  ];
  rr:subjectMap [
    rr:template "http://example.com/parking#{ID}";
    rr:class ns:AparcamientoPublico
  ];

  rr:predicateObjectMap [
    rr:predicate ns:tienePlazas;
    rr:objectMap [
      rml:reference "PLAZAS";
      rr:datatype xsd:integer
    ]
  ];
  rr:predicateObjectMap [
    rr:predicate ns:tieneHorario;
    rr:objectMap [
      rml:reference "HORARIO";
      rr:datatype xsd:string
    ]
  ];
  rr:predicateObjectMap [
    rr:predicate ns:tieneNombre;
    rr:objectMap [
      rml:reference "NOMBRE";
      rr:datatype xsd:string
    ]
  ];
  rr:predicateObjectMap [
    rr:predicate ns:esAccesible;
    rr:objectMap [
      rml:reference "ACCESIBILIDAD";
      rr:datatype xsd:boolean
    ]
  ];
  rr:predicateObjectMap [
    rr:predicate ns:tieneConexion;
    rr:objectMap [
      rml:reference "TRANSPORTE";
      rr:class ns:Transporte
    ]
  ];
  rr:predicateObjectMap [
    rr:predicate ns:estaEnCalle;
    rr:objectMap [
      rml:reference "CALLE";
      rr:class ns:Calle
      ]
  ];
  rr:predicateObjectMap [
    rr:predicate ns:estaEnBarrio;
    rr:objectMap [
      rml:reference "BARRIO";
      rr:class ns:Barrio
      ]
  ];
  rr:predicateObjectMap [
    rr:predicate ns:estaEnDistrito;
    rr:objectMap [
      rml:reference "DISTRITO";
      rr:class ns:Distrito
      ]
  ];

  rr:subjectMap [
    rr:template "http://example.com/parking#{CALLE}";
    rr:class ns:Calle
  ];
  rr:predicateObjectMap [
    rr:predicate ns:estaEnBarrio;
    rr:objectMap [
      rml:reference "BARRIO";
      rr:class ns:Barrio
      ]
  ];
  rr:predicateObjectMap [
    rr:predicate ns:estaEnDistrito;
    rr:objectMap [
      rml:reference "DISTRITO";
      rr:class ns:Distrito
      ]
  ];

  rr:subjectMap [
    rr:template "http://example.com/parking#{BARRIO}";
    rr:class ns:Barrio
  ];
  rr:predicateObjectMap [
    rr:predicate ns:estaEnDistrito;
    rr:objectMap [
      rml:reference "DISTRITO";
      rr:class ns:Distrito
      ]
  ].

